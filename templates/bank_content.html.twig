<table class="table">
  <thead>
    <tr>
      <th>#</th>
      <th>{{ 'username'|trans }}</th>
      <th>{{ 'balance'|trans }}</th>
    </tr>
  </thead>
  <tbody>
    {% for user in users %}
	    <tr>
        <td>{{ user.id }}</td>
        <td>{{ user.username }}</td>
        <td>{{ user.balance }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>